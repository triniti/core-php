{
  "name": "triniti/core",
  "description": "The core Triniti php application and utilities.",
  "type": "library",
  "license": "Apache-2.0",
  "require": {
    "php": ">=8.1",
    "ext-apcu": "*",
    "ext-json": "*",
    "ext-openssl": "*",
    "ext-redis": "*",
    "ext-zlib": "*",
    "aws/aws-sdk-php-symfony": "^2.2",
    "gdbots/app-bundle": "^2.4",
    "gdbots/crypto-bundle": "^3.1",
    "gdbots/enrichments": "^3.0",
    "gdbots/iam-bundle": "^3.0",
    "google/apiclient": "^2.9",
    "guzzlehttp/oauth-subscriber": "~0.5",
    "ruflin/elastica": "^7.1",
    "symfony/asset": "^6.2",
    "symfony/expression-language": "^6.2",
    "symfony/monolog-bundle": "^3.7",
    "symfony/twig-bundle": "^6.2",
    "symfony/web-link": "^6.2",
    "symfony/yaml": "^6.2",
    "gdbots/schemas": "dev-firebase as 3.0.2",
    "triniti/schemas": "dev-firebase as 3.0.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "triniti/acme-schemas": "dev-firebase as 3.0.2"
  },
  "autoload": {
    "psr-4": {
      "Triniti\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Triniti\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  }
}
