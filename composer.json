{
  "name": "triniti/core",
  "description": "The core Triniti php application and utilities.",
  "type": "library",
  "license": "Apache-2.0",
  "require": {
    "php": ">=8.0",
    "aws/aws-sdk-php-symfony": "^2.2",
    "gdbots/app-bundle": "^2.1",
    "gdbots/crypto-bundle": "^2.0",
    "gdbots/enrichments": "^2.0",
    "gdbots/iam-bundle": "^2.1",
    "triniti/schemas": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "triniti/acme-schemas": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Triniti\\": "src"
    }
  },
  "autoload-dev": {
    "files": [
      "tests/Ovp/base-urls.php"
    ],
    "psr-4": {
      "Triniti\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  }
}
