{
  "name": "triniti/core",
  "description": "The core Triniti php application and utilities.",
  "type": "library",
  "license": "Apache-2.0",
  "require": {
    "php": ">=8.0",
    "ext-apcu": "*",
    "ext-json": "*",
    "ext-openssl": "*",
    "ext-redis": "*",
    "ext-zlib": "*",
    "aws/aws-sdk-php-symfony": "^2.2",
    "gdbots/app-bundle": "^2.1",
    "gdbots/crypto-bundle": "^2.0",
    "gdbots/enrichments": "^2.0",
    "gdbots/iam-bundle": "^2.1",
    "guzzlehttp/oauth-subscriber": "^0.5",
    "ruflin/elastica": "^7.1",
    "sensio/framework-extra-bundle": "^5.2",
    "symfony/asset": "^5.2",
    "symfony/expression-language": "^5.2",
    "symfony/monolog-bundle": "^3.5",
    "symfony/twig-bundle": "^5.2",
    "symfony/web-link": "^5.2",
    "symfony/yaml": "^5.2",
    "triniti/schemas": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "triniti/acme-schemas": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Triniti\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Triniti\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  }
}
