{
  "name": "triniti/core",
  "description": "The core Triniti php application and utilities.",
  "type": "library",
  "license": "Apache-2.0",
  "require": {
    "php": ">=8.4",
    "ext-apcu": "*",
    "ext-json": "*",
    "ext-openssl": "*",
    "ext-redis": "*",
    "ext-zlib": "*",
    "aws/aws-sdk-php-symfony": "^2.6",
    "gdbots/app-bundle": "^v3.0",
    "gdbots/crypto-bundle": "^4.0",
    "gdbots/enrichments": "^3.0",
    "gdbots/iam-bundle": "^4.0",
    "google/apiclient": "^2.15",
    "guzzlehttp/oauth-subscriber": "~0.6",
    "ruflin/elastica": "^7.3",
    "symfony/asset": "^7.2 || ^8.0",
    "symfony/expression-language": "^7.2 || ^8.0",
    "symfony/monolog-bundle": "^3.10",
    "symfony/twig-bundle": "^7.2 || ^8.0",
    "symfony/web-link": "^7.2 || ^8.0",
    "symfony/yaml": "^7.2 || ^8.0",
    "triniti/schemas": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^12.0",
    "triniti/acme-schemas": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Triniti\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Triniti\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  }
}
